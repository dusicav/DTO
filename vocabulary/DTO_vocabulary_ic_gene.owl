    <owl:Class rdf:about="&dto;DTO_00219153">
        <rdfs:label>CALHM3</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dto;DTO_00218725"/>
        <symbol>CALHM3</symbol>
