    <!-- http://www.drugtargetontology.org/dto#DTO_00219198 -->

    <owl:Class rdf:about="&dto;DTO_00219198">
        <rdfs:label>FAM26F</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dto;DTO_00218725"/>
