        <symbol>CATSPER4</symbol>
        <entrez_xref>378807</entrez_xref>
    </owl:Class>
    


    <!-- http://www.drugtargetontology.org/dto#DTO_00219196 -->

    <owl:Class rdf:about="&dto;DTO_00219196">
        <rdfs:label>FAM26D</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dto;DTO_00218725"/>
