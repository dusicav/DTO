    <!-- http://www.drugtargetontology.org/dto#DTO_00219197 -->

    <owl:Class rdf:about="&dto;DTO_00219197">
        <rdfs:label>FAM26E</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dto;DTO_00218725"/>
